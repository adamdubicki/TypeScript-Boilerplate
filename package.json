  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.7",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "request": "^2.88.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
